<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.enjoyTrip.auth.dao.LoginDao">

    <select id="login" parameterType="string" resultType="com.ssafy.enjoyTrip.user.dto.UserDto">
        select *
        from user
        where email=#{email}
    </select>

</mapper>